{
  "extends": [
    "airbnb-base",
    "plugin:vue/recommended",
    "@vue/standard"
  ],
  "plugins": [
    "import",
    "vue"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "no-console":"off",
    "max-len":"off",
    "no-plusplus":"off",
    "no-shadow":"off",
    "linebreak-style":"off",
    "global-require":"off",
    "semi": [
      "error",
      "never"
    ],
    "indent":[2, 4]
  }
}